<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity ArtifactCon.
    -->
    <changeSet id="20191004205026-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="the_artifact_id"
                                 baseTableName="artifact_con"
                                 constraintName="fk_artifact_con_the_artifact_id"
                                 referencedColumnNames="id"
                                 referencedTableName="artifact"/>

        <addForeignKeyConstraint baseColumnNames="the_artifact_type_id"
                                 baseTableName="artifact_con"
                                 constraintName="fk_artifact_con_the_artifact_type_id"
                                 referencedColumnNames="id"
                                 referencedTableName="artifact_type"/>

        <addForeignKeyConstraint baseColumnNames="artifact_con_id"
                                 baseTableName="artifact_con_to_multiple_con"
                                 constraintName="fk_artifact_con_to_multiple_con_artifact_con_id"
                                 referencedColumnNames="id"
                                 referencedTableName="artifact_con"/>

        <addForeignKeyConstraint baseColumnNames="to_multiple_con_id"
                                 baseTableName="artifact_con_to_multiple_con"
                                 constraintName="fk_artifact_con_to_multiple_con_to_multiple_con_id"
                                 referencedColumnNames="id"
                                 referencedTableName="multiple_con"/>
        
    </changeSet>
</databaseChangeLog>
